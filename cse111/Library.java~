public class Library{
  public int cap ; int count; public String[] books;
  public Library(int n){ this.cap = n; books = new String[n]}
  public void addBook(String s){
    if(count < books.length()){
      books[count] = s;
      count++;
      System.out.println("Book " + "'" + books[count] + "'" + 
                         " added to the library");
    }
    else{
      System.out.println("Exceeds maximum capacity. You can't add more than" + cap +
                         " books");
    }
  }
  public void printDetail(){
    System.out.println("Maximum Capacity: " + cap);
      System.out.println("Total Books: " + count);
      System.out.println("Book list: ");
                           
      for(int i = 0; i<count; i++){
        System.out.println(books[count]);
      }
  }
        
      }
  }
}