public class Shape{
  public String type;
  public int prop;
  public void setParameters(String s, int n ){
    this.type= s;
    this.prop = n;
  }
  public String details(){
    System.out.println(this.type);
    return "Area: " + Math.square(this.prop, 2) * Math.pi;
  }
    public void setParameters(String s, int n ){
    this.type= s;
    this.prop = n;
  }
}