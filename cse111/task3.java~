import java.util.*;

public class task3{
  public static void main(String [] args) {
    Scanner sc = new Scanner(System.in);
    int size = sc.nextInt();
    int[] arr = new int[size];
    for(int i = 0; i < arr.length; i++){
      arr[i] =sc.nextInt();
    }
    int double = 0;
    for(int i = 0; i < arr.length; i++) {
      for(int j = i+1 ; j < arr.length; j++){
        if ( arr[i] == arr[j]) {
          double++;
        }
      }
    }
    int[] arr2 = new int[size - double];
    int count = 0;
    for(int i =0; i < arr.length; i++) {
      for(int j = i+1 ; j < arr.length; j++){
        if ( arr[i] == arr[j]) {
          count++;
        }
      }
      if( i < arr2.length) {
        arr2[i] = arr[i];
        for(int i = 0; i < arr2.length; i++){
          if(arr2[i] != 0){
            System.out.println(arr2[i] +  count);
          }
        }
        count = 0;
      }
    }
  }
}